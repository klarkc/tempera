{
  "name": "@tempera/stitches",
  "version": "0.1.2",
  "description": "An API that generates twind/style instances from  a set of design tokens.",
  "main": "build/index.js",
  "files": [
    "build"
  ],
  "author": "Mike Mangialardi <mikemangialardi94@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michaelmang/tempera.git"
  },
  "bugs": {
    "url": "https://github.com/michaelmang/tempera/issues"
  },
  "keywords": [
    "design-tokens",
    "twind",
    "twind-style",
    "stitches"
  ],
  "homepage": "https://github.com/michaelmang/tempera",
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.13.8",
    "@babel/preset-env": "^7.13.10",
    "lodash.startcase": "^4.4.0",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.0.3",
    "vendor-copy": "^2.0.0"
  },
  "scripts": {
    "prepublish": "npm run build",
    "clean": "rm -rf build",
    "build": "npm run clean && babel src -d build -s",
    "format": "prettier --write .",
    "copy:plugins": "vendor-copy && rm -rf stubs/tailwindcss/plugins/css",
    "map:plugins-classNames": "babel-node scripts/get-plugin-class-name-mapping.js"
  },
  "dependencies": {
    "lodash.camelcase": "^4.3.0",
    "lodash.kebabcase": "^4.1.1",
    "lodash.merge": "^4.6.2"
  },
  "vendorCopy": [
    {
      "from": "node_modules/tailwindcss/lib/plugins",
      "to": "stubs/tailwindcss/plugins"
    }
  ]
}
